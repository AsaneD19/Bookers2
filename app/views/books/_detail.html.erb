<h1>Book Detail</h1>
<table class="table">
  <tbody>
    <tr>
      <td>
        <%= link_to(user_path(user.id)) do %>
          <%= image_tag user.get_profile_image(100,100) %>
        <% end %>
      </td>
      <td><%= link_to book.title, book_path(book.id) %></td>
      <td><%= book.body %></td>
    </tr>
  </tbody>
</table>


